{
    "version" : "2017-02-28",
    "operation" : "Query",
    "query" : {
        "expression" : "#pk = :pk AND begins_with(#sk,:sk)",
        "expressionNames" : {
            "#pk" : "teamID",
						"#sk" : "userID"
        },
        "expressionValues" : {
						":pk" : $util.dynamodb.toDynamoDBJson($ctx.source.teamID),
            ":sk" : $util.dynamodb.toDynamoDBJson("u")
        }
    },
    "scanIndexForward" : true,
    "consistentRead" : false,
    "select" : "ALL_ATTRIBUTES"
}
